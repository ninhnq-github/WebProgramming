<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>Index.jsp</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.htm</welcome-file>
        <welcome-file>default.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>RedirectionServlet</servlet-name>
        <servlet-class>RedirectionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RedirectionServlet</servlet-name>
        <url-pattern>/GetSubmission</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ass3Servlet</servlet-name>
        <servlet-class>Ass3Servlet</servlet-class>
        <init-param>
            <param-name>pricePerUnit</param-name>
            <param-value>9.95</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>Ass3Servlet</servlet-name>
        <url-pattern>/GetReceipt</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ass4aServlet</servlet-name>
        <servlet-class>Ass4aServlet</servlet-class>
        <init-param>
            <param-name>pricePerUnit</param-name>
            <param-value>9.95</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>Ass4aServlet</servlet-name>
        <url-pattern>/GetReceiptv2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ass4bServlet</servlet-name>
        <servlet-class>Ass4bServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Ass4bServlet</servlet-name>
        <url-pattern>/TakeProcess</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ass4cServlet</servlet-name>
        <servlet-class>Ass4cServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Ass4cServlet</servlet-name>
        <url-pattern>/OrderReply</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ass5Servlet</servlet-name>
        <servlet-class>Ass5Servlet</servlet-class>
        <init-param>
            <param-name>pricePerUnit</param-name>
            <param-value>9.95</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>Ass5Servlet</servlet-name>
        <url-pattern>/GetReceiptv3</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Ass5sServlet</servlet-name>
        <servlet-class>Ass5sServlet</servlet-class>
        <init-param>
            <param-name>pricePerUnit</param-name>
            <param-value>9.95</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>Ass5sServlet</servlet-name>
        <url-pattern>/GetReceiptv4</url-pattern>
    </servlet-mapping>

</web-app>